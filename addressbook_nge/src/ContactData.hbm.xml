<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="com.example.tests.ContactData" table="contact_list">
 <id name="id" column="contact_id" ></id>
 <property name="firstname" column="contact_firstname" />
 <property name="lastname" column="contact_lastname" />
 <property name="address" column="contact_address" />
 <property name="homephone" column="contact_homephone" />
 <property name="mobilephone" column="contact_mobilephone" />
 <property name="workphone" column="contact_workphone" />
 <property name="mail1" column="contact_mail1" />
 <property name="mail2" column="contact_mail2" />
 <property name="bday" column="contact_bday" />
 <property name="bmonth" column="contact_bmonth" />
 <property name="byear" column="contact_byear" />
 <property name="groupfield" column="contact_groupfield" />
 <property name="optionaladdress" column="contact_optionaladdress" />
 <property name="optionalhomephone" column="contact_optionalhomephone" />
 </class>
</hibernate-mapping>

